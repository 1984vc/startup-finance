{
  "name": "cap-table-worksheet",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:hugo": "yarn build && cp dist/assets/index*.js ../assets/cap-table-worksheet/index.js && cp dist/assets/index*.css ../assets/cap-table-worksheet/index.css",
    "lint": "eslint .",
    "format": "prettier . --write",
    "preview": "vite preview",
    "test": "yarn jest --maxWorkers=1 --maxConcurrency=1",
    "build:gas": "rm -rf build && tsc -p tsconfig.gas.json && webpack",
    "deploy:gas": "clasp deploy",
    "gas-login": "clasp login"
  },
  "dependencies": {
    "gas-webpack-plugin": "^2.6.0",
    "lz-string": "^1.5.0",
    "react": "^18.3.1",
    "react-currency-input-field": "^3.8.0",
    "react-dom": "^18.3.1",
    "react-tooltip": "^5.28.0",
    "reselect": "^5.1.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@google/clasp": "^2.4.2",
    "@heroicons/react": "^2.1.5",
    "@jest/globals": "^29.7.0",
    "@types/google-apps-script": "^1.0.83",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.8",
    "tailwindcss-scoped-preflight": "^3.4.3",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "vite-plugin-scope-tailwind": "^2.0.1"
  }
}
