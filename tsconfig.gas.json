{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "gas-build",
    "lib": ["ESNext"],
    "types": ["google-apps-script"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/app/*"],
      "@libray/*": ["./src/library/*"],
    }
  },
  "include": ["src/library"],
  "files": ["src/library/index.ts"]
}
