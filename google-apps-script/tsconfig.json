{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "build",
    "lib": [
      "ESNext"
    ],
    "types": [
      "google-apps-script"
    ],
    "baseUrl": ".",
    "paths": {
      "@library/*": ["../src/library/*"],
    }
  },
  "include": [
    "./src/index.ts"
  ],
  "exclude": [
  ],
}